{
    "name": "phpbbservices/selectivemassemails",
    "type": "phpbb-extension",
    "description": "Sends selective mass emails based on various criteria",
    "homepage": "https://www.phpbbservices.com",
    "version": "1.0.8",
    "time": "2019-10-17",
    "license": "GPL-2.0-only",
    "authors": [
        {
            "name": "Mark D. Hamill",
            "email": "mark@phpbbservices.com",
            "homepage": "https://www.phpbbservices.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4,<7.3",
        "composer/installers": "~1.0"
    },
    "extra": {
        "display-name": "Selective mass emails",
        "soft-require": {
            "phpbb/phpbb": ">=3.2.0,<3.3.0@dev"
        },
        "version-check": {
            "host": "www.phpbbservices.com",
            "directory": "/other/selectivemassemails/updatecheck/",
            "filename": "3.2.json",
            "ssl": true
        }
    }
}
